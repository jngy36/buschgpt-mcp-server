# Docker Environment Configuration for BuschGPT MCP Server
# Copy this file to .env and fill in your actual values

# BuschGPT Authentication Configuration
STATWORX_AUTH_SERVER=https://your-auth-server.com/oauth/token
STATWORX_CLIENT_ID=your_client_id_here
STATWORX_CLIENT_SECRET=your_client_secret_here

# Optional: Node.js Environment
NODE_ENV=production

# Optional: Logging Level
LOG_LEVEL=info